{
  "name": "classroom-APIs",
  "version": "1.0.0",
  "description": "classroom-APIs this is simple application which built by using NODEJS",
  "main": "connection.js",
  "scripts": {
    "lint": "eslint",
    "test": "nyc mocha --exit --require @babel/register --require babel-polyfill ./test/*.js",
    "insert_table": "nodemon --exec babel-node ./server/api/models/database/insert_into_db.js",
    "create_schema": "nodemon --exec babel-node ./server/api/models/database/schema.js",
    "create_table": "nodemon --exec babel-node ./server/api/models/database/table.js",
    "drop_table": "nodemon --exec babel-node ./server/api/models/database/drop.js",
    "connect_db": "nodemon --exec babel-node ./server/api/config/connect_db.js",
    "server": "nodemon --exec babel-node connection.js",
    "start": "babel-node connection.js",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/key-joshua/classroom-APIs.git"
  },
  "author": "Key-Joshua",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "g": "^2.0.1",
    "pg": "^7.12.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "swagger-ui-express": "^4.1.1"
  },
  "devDependencies": {
    "babel-install": "2.1.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2"
  },
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  }
}
